    var bottom = view_yview[0] + view_hview[0];
    draw_rectangle(view_xview[0], bottom - height, view_xview[0] + view_wview[0], bottom, false);

    tX = view_xview[0] + 35;
    tY = bottom - height + height / 2 - 12;
        
    // draw Text
    draw_set_color(c_white);
    draw_text(tX, tY, temp);
}
